<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
    <link rel="stylesheet" href="~/css/laelynn/courses.css">
</head>

<body>
    <header>

        @await Html.PartialAsync("LaePartials/_TopNav")
    </header>
    <main>
        <div class="borderdiv">
            <div class="grid-container">
                <div class="gridnav">

                </div>
                <div class="grid-item1">
                    <h2 class="h2courses">Your current status:</h2>
                    <div class="container">
                        <div class="progress2 progress-moved">
                            <div class="progress-bar2">
                            </div>
                        </div>
                        <div class="row">
                            <h1 class="numbers">128</h1>
                            <h6 class="h6courses">: days in training</h6>
                        </div>
                        <div class="row">
                            <h1 class="numbers">30</h1>
                            <h6 class="h6courses">: completed courses</h6>
                        </div>

                    </div>
                </div>
                <div class="grid-item2">
                    <h2 class="register">
                        YOUR REGISTERED COURSES:
                    </h2>
                    <div class="regcourses">
                        <h4 class="h4courses">~ Intro to Java</h4>
                        <h4 class="h4courses">~ How to Get with Git</h4>
                        <h4 class="h4courses">~ Terminal Commands</h4>
                        <h4 class="h4courses">~ Fundamentals Crash Course</h4>
                    </div>

                </div>
                <div class="grid-item3">
                    <h4 class="schedule">TODAY'S SCHEDULE</h4>
                    <hr>
                    <div class="coursebox">
                        <h6 class="time">12PM</h6>
                        <h6 class="thecourse">HTML/CSS playground</h6>
                    </div>
                    <hr>
                    <div class="coursebox">
                        <h6 class="time">2PM</h6>
                        <h6 class="thecourse">Algos, algos, algos!</h6>
                    </div>
                    <hr>
                    <div class="coursebox">
                        <h6 class="time">4PM</h6>
                        <h6 class="thecourse">Tips & Tricks to Coding Mastery</h6>

                    </div>
                    <hr>
                    <div class="coursebox">
                        <h6 class="time">6PM</h6>
                        <h6 class="thecourse">Time Management & Other Skills</h6>
                    </div>
                </div>
                @* <div class="grid-item2">4</div> *@
                @* <div class="grid-item1">5</div> *@
                @* <div class="grid-item2">6</div> *@
                <div class="grid-item5">
                    <img class="girl" src="https://www.codelikeagirl.com/Assets/CLG/img/CLG_Logo.png">
                </div>
                <div class="grid-item4">
                </div>
            </div>


            <a class="top-link show" href="" id="js-top">
                <svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 12 6">
                    <path d="M12 6H0l6-6z" />
                </svg>
                @* <span class="screen-reader-text">BACK TO TOP</span> *@
            </a>
        </div>
    </main>
</body>

</html>